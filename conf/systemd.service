[Unit]
Description=Minecraft Server
After=network.target

[Service]
Type=simple
User=__APP__
Group=__APP__
Nice=1
KillMode=none
SuccessExitStatus=0 1
ProtectHome=true
ProtectSystem=full
PrivateDevices=true
NoNewPrivileges=true
WorkingDirectory=__FINALPATH__
UMask=000
ExecStart=/usr/lib/jvm/jdk-__JAVA_VERSION__/bin/java -Xmx__RAM__M -Xms__RAM__M -jar server.jar nogui

[Install]
WantedBy=multi-user.target
